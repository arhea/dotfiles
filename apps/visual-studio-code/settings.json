{
  "editor.fontSize": 13,
  "editor.fontFamily": "RobotoMono Nerd Font Monospace, Monaco, 'Courier New', monospace",
  "editor.wordWrap": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "terminal.integrated.fontFamily": "Roboto Mono, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": "13",
  "extensions.autoUpdate": true,
  "terraform.format.enable": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "Monokai Pro (Filter Octagon) Icons",
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [
      ".terraform/**/*",
      "**/.terraform/**/*"
    ]
  },
  "terraform.languageServer": {
    "external": true,
    "args": [
      "serve"
    ]
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "python.jediEnabled": false,
"aws.samcli.location": "/usr/local/bin/sam",
"python.languageServer": "Microsoft",
"editor.formatOnSave": true,
"editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": false
},
"[scss]": {
  "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
},
"[php]": {
  "editor.defaultFormatter": "junstyle.php-cs-fixer"
},
"php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
"php-cs-fixer.lastDownload": 1626282813388,
"go.toolsManagement.autoUpdate": true,
"workbench.colorTheme": "GitHub Dark"
}
